<launch>
  <arg name="joint_control_topic_name" />

  <node pkg="bspline_generator" type="aerial_plannar_node" name="aerial_plannar" output="screen">
    <param name="debug_verbose" value="false" />
    <param name="control_frequency" value="40.0" />

    <!-- trajectory relate -->
    <param name="bspline_degree" value="5" />
    <param name="trajectory_period" value="80.0" />

    <!-- trajectory tracking relate -->
    <param name="joint_upperbound" value="1.58" />
    <param name="joint_lowerbound" value="-1.58" />
    <param name="joint_control_topic_name" value="$(arg joint_control_topic_name)" />

  </node>
</launch>
