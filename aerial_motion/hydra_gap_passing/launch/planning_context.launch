<launch>

  <!-- The semantic description that corresponds to the URDF -->
  <param name="hydra/robot_description_semantic" textfile="$(find hydra_moveit_config)/config/hydra.srdf" />
  
  <!-- Load to the parameter server yaml files -->
  <group ns="robot_description_planning">    
    <rosparam command="load" file="$(find hydra_moveit_config)/config/joint_limits.yaml"/>
  </group>
  
</launch>
